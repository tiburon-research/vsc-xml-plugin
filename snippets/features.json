{
    "ftpPath":{
        "prefix": "@StoreUrl",
        "body":[
            "@StoreUrl/t/tib_${TM_FILENAME/^(\\d+)(.*)$/$1/}/$0"
        ],
        "description": "Стандартный путь к материалам проета"
    },
    "src":{
        "prefix": "[img src=\"",
        "body": [
            "[img src=\"@StoreUrl/t/tib_${TM_FILENAME/^(\\d+)(.*)$/$1/}/$2\"/]"
        ],
        "description": "img с путём к материалам проета"
    },
    "blockFolding":{
        "prefix": "<!--block",
        "body": [
            "<!--#block ${1:Name} -->\n",
            "\t$0",
            "\n<!--#endblock-->"
        ],
        "description": "Сворачиваемый блок"
    },
    "blockLight":{
        "prefix": "<!--*",
        "body": [
            "<!--* ${1:Текст} -->\n"
        ],
        "description": "Зелёный блок"
    },
    "blockWarning":{
        "prefix": "<!--**",
        "body": [
            "<!--** ${1:Текст} -->\n"
        ],
        "description": "Красный блок"
    }

}