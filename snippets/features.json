            "\t<![CDATA[",
            "\t\t<!-- Yandex.Metrika counter -->",
            "\t\t<script type=\"text/javascript\">",
            "\t\tvar yaParams = {'project': '[c#]InterviewPars.ProjectID;[/c#]', 'interview': '[c#]InterviewPars.RespID;[/c#]'};",
            "\t\t(function (d, w, c) {",
            "\t\t\t(w[c] = w[c] || []).push(function() {",
            "\t\t\t\ttry {",
            "\t\t\t\t\tw.yaCounter26653611 = new Ya.Metrika({id:26653611,",
            "\t\t\t\t\t\twebvisor:true,",
            "\t\t\t\t\t\tclickmap:true,",
            "\t\t\t\t\t\ttrackLinks:true,",
            "\t\t\t\t\t\taccurateTrackBounce:true,",
            "\t\t\t\t\t\tparams:window.yaParams||{} });",
            "\t\t\t\t} catch(e) { }",
            "\t\t\t});",
            "\t\t\tvar n = d.getElementsByTagName(\"script\")[0],",
            "\t\t\t\ts = d.createElement(\"script\"),",
            "\t\t\t\tf = function () { n.parentNode.insertBefore(s, n); };",
            "\t\t\ts.type = \"text/javascript\";",
            "\t\t\ts.async = true;",
            "\t\t\ts.src = (d.location.protocol == \"https:\" ? \"https:\" : \"http:\") + \"//mc.yandex.ru/metrika/watch.js\";",
            "\t\t\tif (w.opera == \"[object Opera]\") {",
            "\t\t\t\td.addEventListener(\"DOMContentLoaded\", f, false);",
            "\t\t\t} else { f(); }",
            "\t\t})(document, window, \"yandex_metrika_callbacks\");",
            "\t\t</script>",
            "\t\t<noscript><div><img src=\"//mc.yandex.ru/watch/26653611\" style=\"position:absolute; left:-9999px;\" alt=\"\" /></div></noscript>",
            "\t\t<!-- /Yandex.Metrika counter -->",
            " \t]]>",
            "</CustomText2>"
        ],
        "description": "Яндекс.Метрика"
    },
