{
	"PageFull": {
		"prefix": "<Page",
		"body": [
            "<Page Id=\"${1:Id}\">",
                "\t<Question Id=\"${1:Id}\" Type=\"${2|RadioButton,CheckBox,Text,Memo,Integer,Number,File,Date|}\">",
                    "\t\t<Header>$3</Header>",
                    "\t\t$0",
                "\t</Question>",
            "</Page>\n"
		],
		"description": "Полная структура Page"
    },
    "PageShort": {
		"prefix": "<Page",
		"body": [
            "<Page Id=\"${1:Id}\">",
                "\t<Header>$0</Header>",
            "</Page>\n"
		],
		"description": "Краткая структура Page"
    },
    "Question":{
        "prefix": "<Question",
		"body": [
            "<Question Id=\"${1:Id}\" Type=\"${2|RadioButton,CheckBox,Text,Memo,Integer,Number,File,Date|}\">",
                "\t<Header>$3</Header>",
                "\t$0",
            "</Question>"
		],
		"description": "Структура Question"
    },
    "Block":{
        "prefix": "<Block",
        "body":[
            "<Block Items=\"$1\"/>"
        ],
        "description": "Структура Block"
    },
    "List":{
        "prefix": "<List",
        "body": [
            "<List Id=\"${1:Id}\">",
            "\t$0",
            "</List>"
        ],
        "description": "Структура List"
    },
    "RedirectCode":{
        "prefix": "<Redirect",
        "body": [
            "<Redirect><![CDATA[",
            "\t$0",
            "\treturn false;",
            "]]></Redirect>"
        ],
        "description": "Структура Redirect"
    },
    "RedirectStatus":{
        "prefix": "<Redirect",
        "body": [
            "<Redirect Status=\"${1:19}\">return $0;</Redirect>"
        ],
        "description": "Структура Redirect со статусом"
    },
    "Redirect18":{
        "prefix": "<Redirect",
        "body": [
            "<Redirect Status=\"18\"/>"
        ],
        "description": "Финальный Redirect"
    },
    "Validate":{
        "prefix": "<Validate",
        "body": [
            "<Validate Message=\"Проверьте правильность введённых данных\" PinAnswer=\"\" PinQuestion=\"\">",
            "\treturn $0;",
            "</Validate>"
        ],
        "description": "Структура Validate"
    },
    "Filter": {
        "prefix": "<Filter>",
        "body":[
            "<Filter>return $0;</Filter>"
        ],
        "description": "Структура Filter"
    },
    "CDATA":{
        "prefix": "<![CDATA[",
        "body": [
            "<![CDATA[",
            "\t$0",
            "]]>"
        ],
        "description": "CDATA"
    },
    "CustomText1":{
        "prefix": "<CustomText1",
        "body": [
            "<CustomText1>",
            "\t<![CDATA[",
            "\t\t$0",
            "\t]]>",
            "</CustomText1>"
        ],
        "description": "CustomText1"
    },
    "CustomText2":{
        "prefix": "<CustomText2",
        "body": [
            "<CustomText2>",
            "\t<![CDATA[",
            "\t\t<style>",
            "\t\t\t$0",
            "\t\t</style>",
            "\t]]>",
            "</CustomText2>"
        ],
        "description": "CustomText2 (+style)"
    },
    "Quota":{
        "prefix": "<Quota",
        "body":[
            "<Quota Id=\"$1\" Apply=\"$2\" Limit=\"$3\">",
            "\t$0",
            "</Quota>"
        ],
        "description": "Структура Quota"
    },
    "Methods":{
        "prefix": "<Methods",
        "body": [
            "<Methods>",
            "\t<![CDATA[",
            "\t\t$0",
            "\t]]>",
            "</Methods>"
        ],
        "description": "Структура Methods"
    }
}