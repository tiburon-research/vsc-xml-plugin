{
	"PageFull": {
		"prefix": "<Page",
		"body": [
            "<Page Id=\"${1:Id}\">",
                "\t<Question Id=\"${1:Id}\" Type=\"${2|RadioButton,CheckBox,Text,Memo,Integer,Number,File,Date|}\">",
                    "\t\t<Header>$3</Header>",
                    "\t\t$0",
                "\t</Question>",
            "</Page>\n"
		],
		"description": "Полная структура Page"
    },
    "PageShort": {
		"prefix": "<Page",
		"body": [
            "<Page Id=\"${1:Id}\">",
                "\t<Header>$0</Header>",
            "</Page>\n"
		],
		"description": "Краткая структура Page"
    },
    "Question":{
        "prefix": "<Question",
		"body": [
            "<Question Id=\"${1:Id}\" Type=\"${2|RadioButton,CheckBox,Text,Memo,Integer,Number,File,Date|}\">",
                "\t<Header>$3</Header>",
                "\t$0",
            "</Question>"
		],
		"description": "Структура Question"
    },
    "Answer":{
        "prefix": "<Answer",
        "body": [
            "<Answer Id=\"${1:Id}\"><Text>$0</Text></Answer>"
        ],
        "description": "Структура Answer"
    },
    "Block":{
        "prefix": "<Block",
        "body":[
            "<Block Items=\"$1\"/>"
        ],
        "description": "Структура Block"
    },
    "List":{
        "prefix": "<List",
        "body": [
            "<List Id=\"${1:Id}\">",
            "\t$0",
            "</List>"
        ],
        "description": "Структура List"
    },
    "SexList":{
        "prefix": "_sexList",
        "body": [
            "<List Id=\"sexList\">",
            "\t<Item Id=\"1\" Var=\"Мужчины\"><Text>Man</Text></Item>",
            "\t<Item Id=\"2\" Var=\"Женщины\"><Text>Woman</Text></Item>",
            "</List>"
        ],
        "description": "Структура List"
    },
    "RedirectCode":{
        "prefix": "<Redirect",
        "body": [
            "<Redirect>",
            "\t$0",
            "\treturn false;",
            "</Redirect>"
        ],
        "description": "Структура Redirect"
    },
    "RedirectStatus":{
        "prefix": "<Redirect",
        "body": [
            "<Redirect Status=\"${1:19}\">return $0;</Redirect>"
        ],
        "description": "Структура Redirect со статусом"
    },
    "Validate":{
        "prefix": "<Validate",
        "body": [
            "<Validate Message=\"Проверьте правильность введённых данных\" PinAnswer=\"\" PinQuestion=\"\">",
            "\treturn $0;",
            "</Validate>"
        ],
        "description": "Структура Validate"
    },
    "Filter": {
        "prefix": "<Filter>",
        "body":[
            "<Filter>return $0;</Filter>"
        ],
        "description": "Структура Filter"
    },
    "blockFolding":{
        "prefix": "<!--block",
        "body": [
            "<!--#block ${1:Name}-->\n",
            "\t$0",
            "\n<!--#endblock-->"
        ],
        "description": "Сворачиваемый блок"
    },
    "CDATA":{
        "prefix": "<![CDATA[",
        "body": [
            "<![CDATA[",
            "\t$0",
            "]]>"
        ],
        "description": "CDATA"
    },
    "CustomText1":{
        "prefix": "<CustomText1",
        "body": [
            "<CustomText1>",
            "\t<![CDATA[",
            "\t\t$0",
            "\t]]>",
            "</CustomText1>"
        ],
        "description": "CustomText1"
    },
    "CustomText2":{
        "prefix": "<CustomText2",
        "body": [
            "<CustomText2>",
            "\t<![CDATA[",
            "\t\t<style>",
            "\t\t\t$0",
            "\t\t</style>",
            "\t]]>",
            "</CustomText2>"
        ],
        "description": "CustomText2 (+style)"
    },
    "Repeat":{
        "prefix": "<Repeat",
        "body": [
            "<Repeat ${1|List,Length,Range|}=\"$2\">",
            "\t$0",
            "</Repeat>"
        ],
        "description": "Структура Repeat"
    },
    "Quota":{
        "prefix": "<Quota",
        "body":[
            "<Quota Id=\"$1\" Apply=\"$2\" Limit=\"$3\">",
            "\t$0",
            "</Quota>"
        ]
    },
    "RepeatInline":{
        "prefix": "$repeat",
        "body":[
            "\\$repeat(${1:source}){$2[,]}"
        ]
    },
    "ftpPath":{
        "prefix": "@StoreUrl",
        "body":[
            "@StoreUrl/t/tib_${TM_FILENAME/^(\\d+)(.*)$/$1/}/$0"
        ],
        "description": "Стандартный путь к материалам проета"
    },
    "src":{
        "prefix": "[img src=\"",
        "body": [
            "[img src=\"@StoreUrl/t/tib_${TM_FILENAME/^(\\d+)(.*)$/$1/}/$2\"]"
        ],
        "description": "Стандартный путь к материалам проета"
    }
}